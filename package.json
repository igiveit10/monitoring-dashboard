{
  "name": "url-monitoring-dashboard",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "18.x",
    "npm": "10.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "npx tsx scripts/ensure-data-dirs.ts && (if [ -n \"$DATABASE_URL\" ]; then npx prisma db push --accept-data-loss || true; fi) && npx next build",
    "start": "npx tsx scripts/ensure-data-dirs.ts && (if [ \"$FORCE_SEED_RUNS\" = \"true\" ]; then npx tsx scripts/seed-runs.ts; fi) && npx next start -p $PORT",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:push:force": "prisma db push --accept-data-loss",
    "db:studio": "prisma studio",
    "db:seed": "npx tsx scripts/seed.ts",
    "import:qa": "npx tsx scripts/import-qa-data.ts",
    "postinstall": "prisma generate",
    "ensure-dirs": "npx tsx scripts/ensure-data-dirs.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "csv-parse": "^5.5.6",
    "lucide-react": "^0.427.0",
    "next": "^14.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.5.2",
    "tsx": "^4.21.0",
    "xlsx": "^0.18.5",
    "xlsx-js-style": "^1.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.5",
    "postcss": "^8.4.41",
    "prisma": "^5.19.1",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.5.4"
  }
}
